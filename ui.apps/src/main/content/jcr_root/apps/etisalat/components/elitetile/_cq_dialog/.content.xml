<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="ELite Plan Tile"
    sling:resourceType="cq/gui/components/authoring/dialog"
    width="{Long}750">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <eliteplandata
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <eliteplanlist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <elite
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Text that will appear for the Brand Text."
                                                fieldLabel="Brand Text"
                                                maxlength="{Long}40"
                                                name="./brandText"/>
                                            <producttitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Text that will appear for the product Title."
                                                fieldLabel="Product Title"
                                                maxlength="{Long}40"
                                                name="./productTitle"/>
                                            <packagedetails
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Package Details"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <packageitem
                                                        granite:class="package-nv-details"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Package Item(s)"
                                                        validation="multifield-max-2">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./packageItems">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <file
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                    allowUpload="{Boolean}false"
                                                                    autoStart="{Boolean}false"
                                                                    class="cq-droptarget"
                                                                    fieldLabel="Package Image Asset"
                                                                    fileNameParameter="./package/fileName"
                                                                    fileReferenceParameter="./package/fileReference"
                                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                    multiple="{Boolean}false"
                                                                    name="./package/file"
                                                                    title="Upload Speed Image Asset"
                                                                    uploadUrl="${suffix.path}"
                                                                    useHTML5="{Boolean}true"/>
                                                                <packagetext
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Text that will appear for package text."
                                                                    fieldLabel="Package Text"
                                                                    maxlength="{Long}40"
                                                                    name="./packageText"/>
                                                                <packageunits
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Text that will appear for package units."
                                                                    fieldLabel="Package Unit"
                                                                    maxlength="{Long}40"
                                                                    name="./packageUnit"/>
                                                                <packagesubtitle
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Text that will appear for package sub title."
                                                                    fieldLabel="Package SubTitle"
                                                                    maxlength="{Long}40"
                                                                    name="./packageSubTitle"/>
                                                            </items>
                                                        </field>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            max-items="2"/>
                                                    </packageitem>
                                                </items>
                                            </packagedetails>
                                            <featuredetails
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Feature Description"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <featuredescription
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldDescription="A description to display as the feature description."
                                                        fieldLabel="Feature Description"
                                                        name="./featureDesc"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                                            <format
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <justify
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <lists
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <paraformat
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*">
                                                                <formats jcr:primaryType="nt:unstructured">
                                                                    <default_p
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Paragraph"
                                                                        tag="p"/>
                                                                    <default_h1
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 1"
                                                                        tag="h1"/>
                                                                    <default_h2
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 2"
                                                                        tag="h2"/>
                                                                    <default_h3
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 3"
                                                                        tag="h3"/>
                                                                    <default_h4
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 4"
                                                                        tag="h4"/>
                                                                    <default_h5
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 5"
                                                                        tag="h5"/>
                                                                    <default_h6
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 6"
                                                                        tag="h6"/>
                                                                    <default_blockquote
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Quote"
                                                                        tag="blockquote"/>
                                                                    <default_pre
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Preformatted"
                                                                        tag="pre"/>
                                                                </formats>
                                                            </paraformat>
                                                            <links
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <image
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <findreplace
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <subsuperscript
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <spellcheck
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <misctools
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*">
                                                                <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                                    <chars jcr:primaryType="nt:unstructured">
                                                                        <copyright
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;#169;"/>
                                                                        <trademark
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;#8482;"/>
                                                                        <registered
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;#174;"/>
                                                                        <emDash
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;#8212;"/>
                                                                        <pound
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;#163;"/>
                                                                        <nbsp
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;#160;"/>
                                                                    </chars>
                                                                </specialCharsConfig>
                                                            </misctools>
                                                            <styles
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*">
                                                                <styles jcr:primaryType="nt:unstructured">
                                                                    <strikethrough
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="strikethrough"
                                                                        text="strikethrough"/>
                                                                    <deviderline
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="divider-line"
                                                                        text="divider line"/>
                                                                    <fontgoldbluetile
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="font--productdesc"
                                                                        text="Font-small gold/blue tile"/>
                                                                    <bluecheck
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="icon-checkbox-platinum"
                                                                        text="icon check blue"/>
                                                                    <goldcheck
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="icon-checkbox-gold"
                                                                        text="icon check gold"/>
                                                                    <iconsmile
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="icon-smiles"
                                                                        text="smile icon"/>
                                                                    <iconswitch
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="icon-switch"
                                                                        text="switch icon"/>
                                                                    <iconunlimited
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="icon-unlimited"
                                                                        text="unlimited icon"/>
                                                                    <iconimageadjust
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="iconimage"
                                                                        text="icon image adjust 20x20"/>
                                                                    <oldprice
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="old-price"
                                                                        text="Old Price"/>
                                                                    <oldpricevalue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="old-price-value"
                                                                        text="Old Price Value"/>
                                                                    <spacebottom
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="space-bottom"
                                                                        text="Space Botttom"/>
                                                                    <spaceleft
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="space-left"
                                                                        text="Space Left"/>
                                                                    <spaceright
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="space-right"
                                                                        text="Space Right"/>
                                                                    <spacetop
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="space-top"
                                                                        text="Space Top"/>
                                                                    <buttondisabled
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-button--disabled"
                                                                        text="Button Disabled"/>
                                                                    <buttongrey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-button--grey"
                                                                        text="Button Grey"/>
                                                                    <buttonprimary
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-button--primary"
                                                                        text="Button Primary"/>
                                                                    <buttonsecondary
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-button--secondary"
                                                                        text="Button Secondary"/>
                                                                    <fullwidthbutton
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-button--width_100"
                                                                        text="Secondary Button Full Width"/>
                                                                    <fontbig
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="font--big"
                                                                        text="Font Big"/>
                                                                    <fontmedium
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="font--med"
                                                                        text="Font Medium"/>
                                                                    <fontsmall
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="font--small"
                                                                        text="Font Small"/>
                                                                    <fontxsmall
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="font--xsmall"
                                                                        text="Font X-Small"/>
                                                                    <fontcolorblack
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="text--black"
                                                                        text="Font Color Black"/>
                                                                    <fontcolorgray
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="text--gray"
                                                                        text="Font Color Gray"/>
                                                                    <fontcolorgreen
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="text--green"
                                                                        text="Font Color Green"/>
                                                                    <fontcolororange
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="text--orange"
                                                                        text="Font Color Orange"/>
                                                                    <fontcolorwhite
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="text--white "
                                                                        text="Font Color White"/>
                                                                    <linkarrow
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-link__arrow"
                                                                        text="Link Arrow"/>
                                                                    <whitelinkarrow
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-link__arrow cmp-link__arrow--white"
                                                                        text="White Link Arrow "/>
                                                                    <linkarrowlefticon
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-link__arrow_left"
                                                                        text="Link Arrow Left Icon"/>
                                                                    <linktext
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-link__text"
                                                                        text="Link Text"/>
                                                                    <listarrow
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="cmp-icon__arrow__list"
                                                                        text="List Style Arrow"/>
                                                                    <number
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cssName="font--number"
                                                                        text="Number"/>
                                                                </styles>
                                                            </styles>
                                                            <table
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                        </rtePlugins>
                                                        <uiSettings jcr:primaryType="nt:unstructured">
                                                            <cui jcr:primaryType="nt:unstructured">
                                                                <inline
                                                                    jcr:primaryType="nt:unstructured"
                                                                    toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                                    <popovers jcr:primaryType="nt:unstructured">
                                                                        <justify
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                            ref="justify"/>
                                                                        <lists
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                            ref="lists"/>
                                                                        <paraformat
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                                            ref="paraformat"/>
                                                                        <styles
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="styles:getStyles:styles-pulldown"
                                                                            ref="styles"/>
                                                                    </popovers>
                                                                </inline>
                                                                <dialogFullScreen
                                                                    jcr:primaryType="nt:unstructured"
                                                                    toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                                    <popovers jcr:primaryType="nt:unstructured">
                                                                        <paraformat
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                                            ref="paraformat"/>
                                                                    </popovers>
                                                                </dialogFullScreen>
                                                                <tableEditOptions
                                                                    jcr:primaryType="nt:unstructured"
                                                                    toolbar="[table#insertcolumn-before,table#insertcolumn-after,table#removecolumn,-,table#insertrow-before,table#insertrow-after,table#removerow,-,table#mergecells-right,table#mergecells-down,table#mergecells,table#splitcell-horizontal,table#splitcell-vertical,-,table#selectrow,table#selectcolumn,-,table#ensureparagraph,-,table#modifytableandcell,table#removetable,-,undo#undo,undo#redo,-,table#exitTableEditing,-]"/>
                                                            </cui>
                                                        </uiSettings>
                                                    </featuredescription>
                                                </items>
                                            </featuredetails>
                                            <viewChannels
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="View Channels"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <file
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        allowUpload="{Boolean}false"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fieldDescription="Upload image for &quot;View Channels&quot;"
                                                        fieldLabel="View Channels Image Asset"
                                                        fileNameParameter="./fileName1"
                                                        fileReferenceParameter="./fileReference1"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./file1"
                                                        title="Upload image for View Channels"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                    <packagetext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for &quot;View Channels&quot;"
                                                        fieldLabel="View Channels Text"
                                                        name="./viewChannelsText"/>
                                                </items>
                                            </viewChannels>
                                            <pricetag
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Price Tag"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <productprice
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for product price."
                                                        fieldLabel="Product Price"
                                                        maxlength="{Long}40"
                                                        name="./productPrice"/>
                                                    <oldproductpricetext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for product old price text."
                                                        fieldLabel="Product Old Price Text"
                                                        maxlength="{Long}40"
                                                        name="./productOldPriceText"/>
                                                    <oldproductprice
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for product old price."
                                                        fieldLabel="Product Old Price"
                                                        maxlength="{Long}40"
                                                        name="./productOldPrice"/>
                                                    <productcurrency
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for product currency."
                                                        fieldLabel="Product Currency"
                                                        maxlength="{Long}40"
                                                        name="./productCurrency"/>
                                                    <vatinclude
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Text that will appear for VAT include"
                                                        fieldLabel="VAT Include Text"
                                                        maxlength="{Long}40"
                                                        name="./vatInclude"/>
                                                </items>
                                            </pricetag>
                                            <smilepoints
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Text that will appear for smile points label."
                                                fieldLabel="Smile Points Text"
                                                maxlength="{Long}40"
                                                name="./smilePointsLabel"/>
                                            <buynow
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Text that will appear for the buy now button text"
                                                fieldLabel="Buy Now Text"
                                                maxlength="{Long}40"
                                                name="./availabilityButtonText"/>
                                            <buynowlink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Buy Now Button Link"
                                                fieldLabel="Buy Now Button Link"
                                                name="./availabilityButtonLink"
                                                rootPath="/content"/>
                                            <buynowalttext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="The alt text for Accessibility. This text will appear when the mouse hovers over the link."
                                                fieldLabel="BuyNow Link Alt Text"
                                                maxlength="{Long}40"
                                                name="./buyNowAltText"/>
                                        </items>
                                    </field>
                                </items>
                            </eliteplanlist>
                        </items>
                    </eliteplandata>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
