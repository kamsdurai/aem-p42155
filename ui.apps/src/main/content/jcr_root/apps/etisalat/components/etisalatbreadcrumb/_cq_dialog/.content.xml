<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Etisalat Breadcrumb"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <etisalatbreadcrumbdata
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Etisalat Breadcrumb"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <etisalatbreadcrumb
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Etisalat Breadcrumb">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./etisalatBreadcrumb">
                                    <items jcr:primaryType="nt:unstructured">
                                        <breadcrumbtitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Breadcrumb title"
                                            fieldLabel="Breadcrumb Title"
                                            maxlength="{Long}40"
                                            name="./breadcrumbTitle"/>
                                        <breadcrumblink
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Breadcrumb link "
                                            fieldLabel="Breadcrumb Link"
                                            name="./breadcrumbLink"
                                            rootPath="/content"/>
                                        <linkbehavior
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Link Behavior"
                                            name="./linkBehavior">
                                            <items jcr:primaryType="nt:unstructured">
                                                <opennewwindow
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Open In New Window"
                                                    value="opennewwindow"/>
                                                <opensamewindow
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Open in Same Window"
                                                    value="opensamewindow"/>
                                            </items>
                                        </linkbehavior>
                                    </items>
                                </field>
                            </etisalatbreadcrumb>
                        </items>
                    </etisalatbreadcrumbdata>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
