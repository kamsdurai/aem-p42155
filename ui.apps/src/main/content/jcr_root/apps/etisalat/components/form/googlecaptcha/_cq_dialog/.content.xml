<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Google Capcha"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <captchadata
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Google Captcha"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <captchaversion
                                granite:class="cq-dialog-dropdown-showhide"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Select the Google Captcha Version which you want to show."
                                fieldLabel="Select Captcha Version"
                                name="./captchaVersion">
                                <items jcr:primaryType="nt:unstructured">
                                    <captchav2
                                        jcr:primaryType="nt:unstructured"
                                        text="Captcha V2"
                                        value="captchav2"/>
                                    <captchav3
                                        jcr:primaryType="nt:unstructured"
                                        text="Captcha V3"
                                        value="captchav3"/>
                                    <captchainvisible
                                        jcr:primaryType="nt:unstructured"
                                        text="Captcha Invisible"
                                        value="captchaInvisible"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    cq-dialog-dropdown-showhide-target=".productlist-showhide-target"/>
                            </captchaversion>
                            <captchav2
                                granite:class="productlist-showhide-target"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <captchakeyv2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription=" Google Captcha Key for Version 2."
                                        fieldLabel="Google Captcha Key V2"
                                        maxlength="{Long}10000"
                                        name="./captchaKeyV2"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    showhidetargetvalue="captchav2"/>
                            </captchav2>
                            <captchav3
                                granite:class="productlist-showhide-target"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <captchakeyv3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription=" Google Captcha Key for Version 3."
                                        fieldLabel="Google Captcha Key V3"
                                        maxlength="{Long}10000"
                                        name="./captchaKeyV3"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    showhidetargetvalue="captchav3"/>
                            </captchav3>
                            <captchainvisiblev3
                                granite:class="productlist-showhide-target"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <captchakeyv3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription=" Google Captcha Key for Version 3 Invesible."
                                        fieldLabel="Google Captcha Key V3 Invisible"
                                        maxlength="{Long}10000"
                                        name="./captchaKeyInvisibleV3"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    showhidetargetvalue="captchaInvisible"/>
                            </captchainvisiblev3>
                        </items>
                    </captchadata>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
