<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/etisalat/settings/wcm/templates/hiuapp-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="etisalat/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="etisalat/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etisalat/components/container"
                layout="responsiveGrid">
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="etisalat/components/text"
                    text="&lt;a class=&quot;scrollUp&quot; href=&quot;#top&quot;>&lt;i class=&quot;fa fa-angle-up&quot;>&lt;/i>&lt;/a>"
                    textIsRich="true"/>
            </container>
        </root>
    </jcr:content>
</jcr:root>
